{
  "personalInfo": {
    "name": {
      "label": "Name",
      "maxLength": 20,
      "required": true,
      "type": "text",
      "placeholder": "John"
    },
    "surname": {
      "label": "Surname",
      "maxLength": 20,
      "required": true,
      "type": "text",
      "placeholder": "Doe"
    },
    "title": {
      "label": "Title",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "Fullstack Web Developer"
    },
    "aboutMe": {
      "label": "About Me",
      "maxLength": 500,
      "required": false,
      "type": "textarea",
      "placeholder": "I am ..."
    },

    "location": {
      "label": "Location",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "Random Place, Random Country"
    },
    "email": {
      "label": "Email",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "johndoe@example.com"
    },
    "phone": {
      "label": "Phone",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "000-000-0000"
    },
    "linkedin": {
      "label": "Linkedin",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "linkedin.com/in/johndoe.is.not.real"
    },
    "website": {
      "label": "Website",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "johndoe.example.com"
    },
    "github": {
      "label": "Github",
      "maxLength": 50,
      "required": false,
      "type": "text",
      "placeholder": "github.com/johndoe.is.not.real"
    }
  },
  "education": {
    "degree": {
      "label": "Degree",
      "maxLength": 50,
      "required": true,
      "type": "text",
      "placeholder": "B.Sc. in Software Engineering"
    },
    "institution": {
      "label": "Institution",
      "maxLength": 50,
      "required": true,
      "type": "text",
      "placeholder": "University of Random City"
    },
    "address": {
      "label": "Address",
      "maxLength": 50,
      "required": true,
      "type": "text",
      "placeholder": "Random City"
    },
    "fromToDate": {
      "type": "date-month-year-interval",
      "fromDateProp": "fromDate",
      "toDateProp": "toDate"
    },
    "fromDate": {
      "type": "date-month-year",
      "label": "From",
      "required": true
    },
    "toDate": {
      "type": "date-month-year",
      "label": "To",
      "required": true
    }
  }
}
